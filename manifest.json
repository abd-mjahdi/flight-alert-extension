{
    "manifest_version":3,
    "name":"SkyAlert",
    "version":"1.0",
    "description" : "notifies when an airplane is near your location",
    "background":{
        "service-worker":"background.js"
    },
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
    }
}